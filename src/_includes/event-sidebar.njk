<nav class="post-sidebar">

  <h3 class="post-sidebar__title">Gli ultimi eventi</h3>
  <ul class="post-sidebar__list">
    {% for post in collections.eventi | published | reverse | limit(8) %}
      <li class="post-sidebar__list-element">
        <a href="{{ post.url }}">{{post.data.title}}</a>
      </li>
    {% endfor %}
  </ul>
</nav>

<hr aria-hidden="true"/>

